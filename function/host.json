{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "extensions": {
    "queues": {
        "maxPollingInterval": "00:01:00",
        "visibilityTimeout" : "00:10:00",
        "batchSize": 1,
        "maxDequeueCount": 5
    }
}

}